{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "buildCommand": "npm run vercel-build",
    "routes": [
      {
        "src": "/(.*\\.(svg|png|jpg|jpeg|gif|ico|webp))",
        "dest": "/$1"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }